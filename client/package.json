{
  "name": "vscode-dasm",
  "displayName": "Dasm macro assembler for VSCode",
  "description": "Develop and run Atari games from inside Visual Studio Code",
  "version": "4.0.0",
  "author": "Zeh Fernando",
  "publisher": "zehfernando",
  "preview": true,
  "license": "GPL-2.0",
  "engines": {
    "vscode": "^1.13.0"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#293134",
    "theme": "dark"
  },
  "browser": {
    "fs": false
  },
  "homepage": "https://github.com/zeh/vscode-dasm/tree/master/client",
  "bugs": {
    "url": "https://github.com/zeh/vscode-dasm/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zeh/vscode-dasm.git"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Linters",
    "Debuggers",
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:dasm",
    "onCommand:extension.vscode-dasm.provideInitialConfigurations"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "breakpoints": [
      {
        "language": "dasm"
      }
    ],
    "debuggers": [
      {
        "type": "dasm",
        "label": "dasm Debug",
        "program": "./out/src/debugger.js",
        "runtime": "node",
        "variables": {},
        "languages": [
          "dasm"
        ],
        "startSessionCommand": "extension.vscode-dasm.openPlayerWindow",
        "configurationSnippets": [
          {
            "label": "dasm Debug: Launch",
            "description": "A new configuration for launching a dasm debug program",
            "body": {
              "type": "dasm",
              "request": "launch",
              "name": "${2:Launch Program}",
              "program": "^\"\\${workspaceRoot}/${1:Program}\""
            }
          }
        ],
        "configurationAttributes": {
          "launch": {
            "required": [],
            "properties": {
              "program": {
                "type": "string",
                "description": "Absolute path to a text file.",
                "default": "${workspaceRoot}/name.asm"
              },
              "stopOnEntry": {
                "type": "boolean",
                "description": "Automatically stop after launch.",
                "default": true
              },
              "trace": {
                "type": "boolean",
                "description": "Enable logging of the Debug Adapter Protocol.",
                "default": true
              }
            }
          }
        },
        "initialConfigurations": "extension.vscode-dasm.provideInitialConfigurations"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "VSCode-dasm configuration",
      "properties": {
        "vscode-dasm.preferUppercase": {
          "type": "array",
          "default": [
            "all"
          ],
          "description": "Controls which language tokens should be uppercase (instructions|symbols|registers|pseudoops|all)."
        },
        "vscode-dasm.trace.server": {
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "Traces the communication between VSCode and the dasm language server service."
        }
      }
    },
    "languages": [
      {
        "id": "dasm",
        "aliases": [
          "Dasm",
          "Assembly",
          "6502"
        ],
        "extensions": [
          ".dasm",
          ".asm",
          ".a",
          ".h"
        ],
        "configuration": "./syntaxes/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dasm",
        "scopeName": "source.asm.dasm",
        "path": "./syntaxes/dasm.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run player:build && tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "lint": "tslint ./src/**/*.ts ./src/**/*.tsx -t stylish --project tsconfig.json --type-check",
    "player:dev": "webpack-dev-server --config player.webpack.config.js -d --host 0.0.0.0 --inline --hot --progress --colors",
    "player:build": "cross-env NODE_ENV=production webpack --config player.webpack.config.js --progress --colors --display-error-details -p"
  },
  "bin": {
    "tslint": "./node_modules/tslint/bin/tslint"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.17",
    "@types/screenfull": "^3.2.0",
    "@types/seedrandom": "^2.4.27",
    "@types/ws": "^3.0.2",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.2.0",
    "babel-preset-es2015": "^6.22.0",
    "clean-webpack-plugin": "^0.1.15",
    "copy-webpack-plugin": "^4.0.1",
    "cpy-cli": "^1.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.4.1",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.0.1",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2",
    "uglify-js": "^2.8.23",
    "url-loader": "^0.5.8",
    "vscode-debugadapter-testsupport": "^1.22.0",
    "webpack": "^3.4.1",
    "webpack-comment-remover-loader": "^0.0.3",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "dasm": "^5.0.1",
    "microevent.ts": "^0.1.0",
    "path": "^0.12.7",
    "screenfull": "^3.3.1",
    "seedrandom": "^2.4.3",
    "setimmediate2": "^3.0.0",
    "simplesignal": "^3.1.0",
    "vscode": "^1.1.4",
    "vscode-debugadapter": "^1.22.0",
    "vscode-debugprotocol": "^1.22.0",
    "vscode-languageclient": "^3.2.2",
    "ws": "^3.1.0"
  }
}
